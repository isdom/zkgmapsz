<zk xmlns:w="client">
	<window height="500px" width="500px" border="normal">
	<popup id="ap">
		<button label="at_pointer" onClick='Clients.showNotification("button clicked")' />
	</popup>
	<popup id="fp">
		<button label="after_pointer" onClick='Clients.showNotification("button clicked")' />
	</popup>
	<popup id="eb">
		<button label="end_before" onClick='Clients.showNotification("button clicked")' />
	</popup>
	<popup id="xy">
		<button label="x=100, y=100" onClick='Clients.showNotification("button clicked")' />
	</popup>
	Move to the gmarker, should see the popup.
	<gmaps id="map" lat="22" lng="22" width="400px" height="400px"
		style="position: absolute; left: 50px; top: 50px;">
		<gmarker lat="21.99" lng="22" tooltip="xy, x=100, y=100"/>
		<gmarker lat="22" lng="22" tooltip="eb, end_before"/>
		<gmarker lat="22.01" lng="22" tooltip="fp, after_pointer"/>
		<gmarker lat="22.02" lng="22" tooltip="ap, at_pointer"/>
	</gmaps>
	</window>
</zk>